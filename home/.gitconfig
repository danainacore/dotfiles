[user]
	name = obcat
	email = obcat@icloud.com
[core]
	excludesFile = ~/.gitignore_global
	autocrlf = input
	editor = vim
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
	keepBackup = false
[alias]
	graph = log --all --decorate --oneline --graph
	ss = status --short
	today = graph --since='24 hour ago'
	unstage = restore --staged
[commit]
	verbose = true
